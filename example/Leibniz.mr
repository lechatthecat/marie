// for confirming the performance. The following function is very slow.
fn a () {
let mut s = 0;
let aaa = 10**5;
print(aaa);
for (let mut i = 0; i <= aaa; i = i + 1) {
    let a = (2*i + 1);
    if (i % 2 == 0) {
        s = s + 1 / a;
    } else {
        s = s - 1 / a;
    }
}

print(4*s);
}

a();