fn fib(n) {
  if (n < 2) return n;
  return fib(n - 1) + fib(n - 2);
}

let before = clock();
print(fib(15));
let after = clock();
print(after - before << "ms");

let mut fact = 1;
for (let mut i = 1; i <= 10; i = i + 1) {
    fact = fact * i;
}
print(fact);

fn fact(n) {
    if (n <= 1) { return 1; }
    return n * fact(n - 1);
}

print(fact(10));
